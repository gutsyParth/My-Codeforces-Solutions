import array

primeList = [True] * (1000001)

for i in range(2, 1000001):
    if primeList[i] == True:
        for j in range(2 * i, 1000001, i):
            primeList[j] = False

n = int(input())

for i in range(n - 1, 0, -1):
    if primeList[i] == False and primeList[n - i] == False:
        print(f"{i} {n-i}")
        break
